<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java.MainNavigationController">
   <children>
      <Button fx:id="addingmethodbutton" layoutX="219.0" layoutY="85.0" mnemonicParsing="false" onAction="#onAddingMethodButton" text="Adding" />
      <Button fx:id="deletingmethodbutton" layoutX="216.0" layoutY="143.0" mnemonicParsing="false" onAction="#onDeletingMethodButton" text="Deleting" />
      <Button fx:id="updatingmethodbutton" layoutX="213.0" layoutY="198.0" mnemonicParsing="false" onAction="#onUpdatingMethodButton" text="Updating" />
      <Button fx:id="viewingmethodbutton" layoutX="216.0" layoutY="257.0" mnemonicParsing="false" onAction="#onViewingMethodButton" text="Viewing" />
      <Button fx:id="randomselectionbutton" layoutX="223.0" layoutY="360.0" mnemonicParsing="false" onAction="#onRandomSelectionClickButton" text="Random Selection" />
      <Label layoutX="56.0" layoutY="307.0" text="If you are done adding, deleting and updating details,  please move on to random selection. " />
      <Label layoutX="15.0" layoutY="332.0" text="Please note that after you proceed to random selection, you cannot add, update or delete anymore details." />
      <Label layoutX="219.0" layoutY="28.0" text="Main Navigation" underline="true">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="38.0" layoutY="89.0" text="Add More Project Details : " />
      <Label layoutX="38.0" layoutY="147.0" text="Delete Project Details :" />
      <Label layoutX="36.0" layoutY="202.0" text="Update Project Details :" />
      <Label layoutX="41.0" layoutY="262.0" text="View Project Details :" />
   </children>
</AnchorPane>
